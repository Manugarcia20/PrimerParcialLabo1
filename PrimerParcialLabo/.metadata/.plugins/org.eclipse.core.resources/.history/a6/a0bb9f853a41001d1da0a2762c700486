#ifndef EDITORIAL_H_
#define EDITORIAL_H_

#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define LLENO 1
#define VACIO 0
#define TAM_E 5

typedef struct{
	int idEditorial;
	char descEditorial[51];
	int estado;
}eEditorial;


void HardcodearEditorial(eEditorial editoriales[]);
void MostrarEditorial(eEditorial editoriales[], int tamE);


#endif /* EDITORIAL_H_ */
